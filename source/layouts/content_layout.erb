	<!-- CSS STYLES -->
	<link href="stylesheets/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
	<link href="stylesheets/css/prettyPhoto.css" rel="stylesheet" type="text/css" />
	<link href="stylesheets/css/flexslider.css" rel="stylesheet" type="text/css" />
	<link href="stylesheets/css/animate.css" rel="stylesheet" type="text/css" media="all" />
	<link href="stylesheets/css/style.css" rel="stylesheet" type="text/css" />
    
	<!-- FONTS -->
	<link href='http://fonts.googleapis.com/css?family=Raleway:400,300,500,600,700,800,900' rel='stylesheet' type='text/css'>
    
